{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/virtual_mem",
        "${workspaceFolder}/src/vjson",
        "${workspaceFolder}/node_modules",
        "${workspaceFolder}/node_modules/node-addon-api",
        "${workspaceFolder}/node_modules/node-addon-api/include",
        "${workspaceFolder}/node_modules/nan",
        "/usr/include/node",
        "${workspaceFolder}/**"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "linux-gcc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}/src",
          "${workspaceFolder}/**"
        ],
        "limitSymbolsToIncludedHeaders": false,
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
      }
    }
  ],
  "version": 4
}
